save
mark
(NotInstalled)
userdict /86f6369627 known {
	userdict /86f6369627 get 
	/perifdict get /inslot get dup type /arraytype eq {
	{
		dup 8 eq {pop pop (LCT) dup exit} if
		dup 2 eq {(1Cassette) exit} if
		dup 7 eq {(2Cassette) exit} if
		pop
	} forall
	dup (1Cassette) eq
	{
	exch pop exch
	{
		7 eq {pop (3Cassette)} if
	} forall
	} {
	dup (2Cassette) eq
	{
	exch pop exch
	{
		2 eq {pop (3Cassette)} if
	} forall
	} {pop} ifelse
	} ifelse
	}{pop} ifelse
}if
= flush
cleartomark
restore
